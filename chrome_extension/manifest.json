{
    "manifest_version": 3,
    "name": "Intelligent Tab Organizer",
    "version": "1.0",
    "description": "AI-powered tab organization",
    "permissions": ["tabs", "tabGroups", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }